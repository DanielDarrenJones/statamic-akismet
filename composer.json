{
    "name": "edalzell/mailchimp",
    "type": "statamic-addon",
    "require": {
        "drewm/mailchimp-api": "^2.5",
        "daftspunk/laravel-config-writer": "^1.3"
    },
    "require-dev": {
        "statamic/cms": "3.0.*@beta",
        "php": "^7.4",
        "laravel/framework": "^6.0|^7.0",
        "laravel/tinker": "^2.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "description": "Subscribe registrations or contact forms to Mailchimp",
    "authors": [
        {
            "name": "Erin Dalzell",
            "email": "erin@silentz.co",
            "homepage": "https://silentz.co",
            "role": "Founder"
        }
    ],
    "autoload": {
        "psr-4": {
            "Edalzell\\Mailchimp\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Mailchimp",
            "description": "Mailchimp addon"
        },
        "laravel": {
            "providers": [
                "Edalzell\\Mailchimp\\ServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/edalzell/laravel-config-writer"
        }
    ]
}
